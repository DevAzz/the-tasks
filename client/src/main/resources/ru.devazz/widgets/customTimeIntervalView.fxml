<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import jfxtras.scene.control.LocalDateTimeTextField?>

<HBox fx:id="customFilterDatePanel" prefWidth="423.0" stylesheets="@../../../../../css/main.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.siencesquad.hqtasks.ui.widgets.CustomTimeIntervalView">
   <children>
      <Button fx:id="clearCustomTimeintervalButton" mnemonicParsing="false" onAction="#clear" prefHeight="24.0" prefWidth="24.0" styleClass="clearTimeIntervalButton">
         <HBox.margin>
            <Insets right="5.0" top="5.0" />
         </HBox.margin>
      </Button>
      <Label text="От" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="5.0" top="8.0" />
         </HBox.margin>
      </Label>
      <LocalDateTimeTextField fx:id="startDateTimeIntervalTextField" prefHeight="25.0" prefWidth="151.0" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="5.0" top="5.0" />
         </HBox.margin>
      </LocalDateTimeTextField>
      <Label text="До" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="5.0" top="8.0" />
         </HBox.margin>
      </Label>
      <LocalDateTimeTextField fx:id="endDateTimeIntervalTextField" prefHeight="25.0" prefWidth="151.0" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="5.0" top="5.0" />
         </HBox.margin>
      </LocalDateTimeTextField>
      <Button fx:id="filterTimeIntervalButton" mnemonicParsing="false" onAction="#search" prefHeight="24.0" prefWidth="24.0" styleClass="searchTasksFilterButton" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="5.0" top="5.0" />
         </HBox.margin></Button>
   </children>
</HBox>
